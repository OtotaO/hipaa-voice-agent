  "entities": {
    "medication": {
      "type": "medication_entity",
      "extraction": "RxNorm lookup",
      "attributes": ["name", "dose", "unit", "route", "frequency", "duration", "indication"]
    },
    "lab_name": {
      "type": "lab_entity",
      "extraction": "LOINC codes",
      "examples": ["CBC", "BMP", "A1C", "lipid panel", "TSH", "troponin"]
    },
    "vital_type": {
      "type": "vital_entity",
      "examples": ["blood pressure", "heart rate", "temperature", "weight", "BMI", "oxygen saturation"]
    },
    "timeframe": {
      "type": "temporal_entity",
      "extraction": "relative and absolute dates",
      "examples": ["today", "last 3 months", "since January", "past week", "previous visit"]
    },
    "section": {
      "type": "note_section",
      "values": ["chief complaint", "HPI", "ROS", "physical exam", "assessment", "plan", "PMH", "medications", "allergies"]
    },
    "diagnosis": {
      "type": "diagnosis_entity",
      "extraction": "ICD-10 lookup",
      "attributes": ["code", "description", "status", "date"]
    },
    "priority": {
      "type": "order_priority",
      "values": ["stat", "urgent", "routine", "future"]
    },
    "specialty": {
      "type": "medical_specialty",
      "examples": ["cardiology", "endocrine", "GI", "pulmonary", "nephrology", "neurology"]
    }
  },
